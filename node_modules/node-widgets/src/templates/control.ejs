<div>
  <% if(value) { %>
    <label for="<%= id %>"><%= label %></label>
    <br>
    <% for(var i = 0; i<value.length;i++) { %>
        <div class="form-group">
          <label>
            <input type="<%= type %>" value="<%= value[i].value%>" class="form-control <%= className %>" style="width: 100%" name="<%= name %>" <%= value[i].checked || '' %> ><%= value[i].name %>
          </label>
        </div>
    <% } %>
  <% } %>
</div>
<br>
